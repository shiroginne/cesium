<h1>Admins</h1>
<ul class="pages_tree">
<% @admins.each do |admin| %>
  <li>
    <div class="info">
      <%= link_to admin.login, edit_admin_admin_url(admin) %>
      <div class="actions">
        <%= link_to 'destroy', admin_admin_url(admin), :method => :delete, :confirm => 'Are u shure?', :class => 'remove_entry' %>
      </div>
    </div>
  </li>
<% end %>
</ul>
<div class="add_entry">
  <%= link_to 'new admin', new_admin_admin_url, :class => 'new_entry' %>
</div>