<h1>Edit page part</h1>
<% form_for @page_part, :url => admin_page_page_part_url(@page, @page_part) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>
<% content_for :additional_navigation do %>
<ul class="buttons">
  <li><%= link_to h(@page.title), edit_admin_page_path(@page) %></li>
  <%= page_parts_links @page, @page_part %>
  <li><%= link_to "Add page part", new_admin_page_page_part_path(@page) %></li>
</ul>
<% end %>
