<h1>New page part</h1>
<% form_for @page_part, :url => admin_page_page_parts_url(@page) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>
<% content_for :additional_navigation do %>
  <%= link_to 'Edit page', edit_admin_page_path(@page) %>
  <%= link_to 'New part', new_admin_page_page_part_path(@page), :class => "selected" %>
  <% @page.page_parts.each do |part| %>
    <%= link_to part.name, edit_admin_page_page_part_path(@page, part) %>
  <% end %>
<% end %>
