page << <<-SCRIPT
  var pages = {#{@pages.map{ |page| "#{page.id}: \"#{page.path}\"" }.join(',')}}
  $H(pages).each(function(pair) {
    $$('#page_' + pair.key + ' .view_entry')[0].href = pair.value
  });
  $$('#page_#{@page.id} .page_name')[0].update('#{ link_to @page.title, edit_admin_page_path(@page) }');
SCRIPT
